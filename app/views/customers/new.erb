<!DOCTYPE HTML>
<html>
    <head>
        <h1>Create Account</h1>
    </head>

    <body>
        <%= form_for @customer do |c| %>
            <%= c.label "Choose a Username" %>
            <%= c.text_field :name %>
            <%= c.label "Enter your telephone number" %>
            <%= c.text_field :tel_num %>
            <%= c.submit "Create Account" %>
        <% end %>
    </body>

</html>